{
    "license": "MIT",
    "require": {
        "php": "^8.0.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.7.1",
        "beberlei/doctrineextensions": "^1.3",
        "composer/package-versions-deprecated": "1.11.99.3",
        "controleonline/financial": "dev-master",
        "controleonline/products": "dev-master",
        "controleonline/users": "dev-master",
        "ctodobom/api-inter": "dev-master",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.7.4",
        "dompdf/dompdf": "^1.0",
        "fpdf/fpdf": "^1.83",
        "frete-click/php-sigep": "*",
        "freteclick/sdk": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "league/oauth2-facebook": "^2.2",
        "league/oauth2-google": "^4.0",
        "league/oauth2-instagram": "^3.1",
        "mautic/api-library": "^2.16",
        "nelmio/cors-bundle": "^2.1",
        "nfephp-org/sped-cte": "dev-master",
        "nfephp-org/sped-da": "^0.1",
        "nfephp-org/sped-nfe": "^5.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpoffice/phpspreadsheet": "^1.18",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.19",
        "symfony/framework-bundle": "5.4.*",
        "symfony/mailer": "*",
        "symfony/messenger": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*",
        "tecnickcom/tcpdf": "^6.4",
        "vich/uploader-bundle": "^1.10",
        "webonyx/graphql-php": "^14.11"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "justinrainbow/json-schema": "^5.2",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/http-client": "5.4.*",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "^5.4",
        "symfony/web-profiler-bundle": "^5.4"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "component-dir": "public/vendor",
        "allow-plugins": {
            "symfony/flex": true,
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "pre-install-cmd": [
            "echo '{\"directory\" : \"public/vendor/\"}' > .bowerrc"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*",
            "docker": false
        }
    }
}
